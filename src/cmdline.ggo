package "peeves"
version "0.0.0"

option "header"   - "Message header text" string required
option "in-addr"  - "Input address" string required
option "in-port"  - "Input port number" int required
option "out-addr" - "Output address" string required
option "out-port" - "Output port number" int required
option "log"      l "Path to log file" string optional
